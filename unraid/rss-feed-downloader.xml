<?xml version="1.0"?>
<Container version="2">
    <Name>rss-feed-downloader</Name>
    <Repository>tonyhsu17/rss-feed-downloader:stable</Repository>
    <Registry>https://hub.docker.com/r/tonyhsu17/rss-feed-downloader/</Registry>
    <Network>bridge</Network>
    <MyIP/>
    <Shell>sh</Shell>
    <Privileged>false</Privileged>
    <Support>https://github.com/tonyhsu17/RssFeedDownloader</Support>
    <Project>https://github.com/tonyhsu17/RssFeedDownloader</Project>
    <Overview>Download files through rss feed</Overview>
    <Category>Downloaders</Category>
    <WebUI/>
    <TemplateURL>https://raw.githubusercontent.com/tonyhsu17/RssFeedDownloader/refs/heads/master/unraid/rss-feed-downloader.xml</TemplateURL>
    <Icon>https://upload.wikimedia.org/wikipedia/en/thumb/4/43/Feed-icon.svg/128px-Feed-icon.svg.png</Icon>
    <ExtraParams/>
    <PostArgs/>
    <CPUset/>
    <DateInstalled>1738651252</DateInstalled>
    <DonateText/>
    <DonateLink/>
    <Requires/>
    <Config Name="RSS_URL" Target="RSS_URL" Default="" Mode="" Description="Url to read from" Type="Variable" Display="always" Required="true" Mask="false"></Config>
    <Config Name="RSS_USE_CRON" Target="RSS_USE_CRON" Default="true" Mode="" Description="Keep job running, set to false for single run" Type="Variable" Display="always" Required="false" Mask="false"></Config>
    <Config Name="RSS_CRON_INTERVAL" Target="RSS_CRON_INTERVAL" Default="600" Mode="" Description="Time in seconds" Type="Variable" Display="always" Required="false" Mask="false"></Config>
    <Config Name="Destination" Target="/data" Default="" Mode="rw" Description="path to mount, don't change container path" Type="Path" Display="always" Required="true" Mask="false"></Config>
    <Config Name="RSS_DES" Target="RSS_DES" Default="/data" Mode="" Description="destination within container" Type="Variable" Display="advanced" Required="true" Mask="false"></Config>
</Container>
